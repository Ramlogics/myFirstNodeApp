<%- include header %>
      
        <div class="login">
            <header class="masthead text-center text-white">
              <div class="masthead-content">
                <div class="container">
                    <div class="wrap-login100">
                        <form class="login100-form validate-form">
                            <span class="login100-form-logo">
                                <i class="fa fa-area-chart"></i>
                            </span>

                            <span class="login100-form-title p-b-34 p-t-27">
                                Register
                            </span>

                            <div class="input-group">
                              <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                              <input id="fullname" type="text" class="form-control wrap-input100" name="fullname" placeholder="fullname">
                            </div>
                            
                            <div class="input-group">
                              <span class="input-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
                              <input id="email" type="text" class="form-control wrap-input100" name="email" placeholder="email">
                            </div>
                            
                            <div class="input-group">
                              <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
                              <input id="password" type="password" class="form-control wrap-input100" name="password" placeholder="Password">
                            </div>
                            
                            <div class="input-group">
                              <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
                              <input id="cpassword" type="password" class="form-control wrap-input100" name="cpassword" placeholder="Confirm Password">
                            </div>

                            <div class="checkbox">
                                <label><input type="checkbox" value="">I agree to the terms & conditions</label>
                            </div>

                            <div class="container-login100-form-btn">
                                <button class="login100-form-btn">
                                    Register
                                </button>
                            </div>
                            
                            <div class="text-center p-t-90">
                                <p class="txt1" href="#">
                                    Already have an account? <a href="login.html">Sign In</a>
                                </p>
                            </div>
                        </form>
                    </div>
                </div>
              </div>
              <div class="bg-circle-1 bg-circle"></div>
              <div class="bg-circle-2 bg-circle"></div>
              <div class="bg-circle-3 bg-circle"></div>
              <div class="bg-circle-4 bg-circle"></div>
            </header>
        </div>
    
<%- include footer %>